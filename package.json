  "scripts": {
    "start": "PORT=8080 REACT_APP_API_CONFIG=dev react-scripts start",
    "build:stage": "REACT_APP_API_CONFIG=stage react-scripts build",
    "deploy:stage": "git push -u origin-staging staging && npm run build:stage && node src/deploy/stage.js",
    "build:prod": "REACT_APP_API_CONFIG=prod react-scripts build",
    "deploy:prod": "git push -u origin-production master && npm run build:prod && node src/deploy/prod.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
