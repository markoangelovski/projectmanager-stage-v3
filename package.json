  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "apexcharts": "^3.20.0",
    "bootstrap": "^4.5.1",
    "easy-peasy": "^3.3.1",
    "gh-pages": "^3.1.0",
    "moment": "^2.27.0",
    "react": "^16.13.1",
    "react-apexcharts": "^1.3.7",
